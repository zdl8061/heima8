@model IEnumerable<Heima8.OA.Model.WF_Instance>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>ShowMyCheck</title>
    <link href="../../Content/tableStyle.css" rel="stylesheet" />
</head>
<body>

    <table>
        <tr>
            <th>流程名</th>
            <th>模板名
            </th>
            
            <th>提交人
            </th>
            <th>提交时间
            </th>
            <th>状态
            </th>
            <th>备注
            </th>
            <th>操作</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
              
                <td>
                    @Html.DisplayFor(modelItem => item.InstanceName)
                </td>
                  <td>
                   @item.WF_Temp.TepName
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.StartBy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StartTime)
                </td>
                <td>
                    @if (item.Status == 0)
                    {
                        <span>审批中</span>
                    }
                    else
                    {
                        <span>结束</span>
                    }
                </td>
              
                <td>
                    @Html.DisplayFor(modelItem => item.Remark)
                </td>
             
                <td>
                    <a href="/WFInstance/ShowInstance/@item.ID">查看</a>
                </td>
            </tr>
        }

    </table>
</body>
</html>
